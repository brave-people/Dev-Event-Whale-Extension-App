{
    "manifest_version": 2,
    "name": "개발자 행사 (데브 이벤트)",
    "version": "1.0.0",
    "description": "개발자 행사를 알려드립니다! ",
    "default_locale": "ko",
    "web_accessible_resources": [
        "static/css/*.css",
        "images/*.png"
    ],
    "content_security_policy": "script-src 'self' https://github.com/brave-people/Dev-Event-Whale-Extension-App ",
    "sidebar_action": {
        "default_page": "static/index.html",
        "default_icon": {
            "16": "images/app-icon.png"
        },
        "default_title": "개발자 행사 (데브 이벤트)",
        "use_navigation_bar": false
    }
}